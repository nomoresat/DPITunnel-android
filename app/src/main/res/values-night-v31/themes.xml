<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DPITunnelcli" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@android:color/system_accent1_200</item>
        <item name="colorPrimaryVariant">@android:color/system_accent1_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@android:color/system_accent2_200</item>
        <item name="colorSecondaryVariant">@android:color/system_accent2_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="toolbarStyle">@style/Widget.MaterialComponents.Toolbar.Surface</item>
    </style>
</resources>